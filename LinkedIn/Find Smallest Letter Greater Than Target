class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        
        
        int left=0;
        int right=letters.length;
        int mid=0;
        
        while(left<right)
        {
            mid=left+(right-left)/2;
            if(letters[mid]<=target)
                left=mid+1;
            else
                right=mid;
        }
        
        return letters[left%letters.length];
      
        
    }
}
        
        
 Time - O(N)
 Binary Search
 
Input:
letters = ["c", "f", "j"]
target = "j"
Output: "c"

Input:
letters = ["c", "f", "j"]
target = "k"
Output: "c"

   
